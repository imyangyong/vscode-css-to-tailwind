{
  "publisher": "imyangyong",
  "name": "css-to-tailwind",
  "displayName": "CssToTailwind",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "CssToTailwind for VS Code",
  "license": "MIT",
  "homepage": "https://github.com/imyangyong/vscode-css-to-tailwind",
  "repository": {
    "type": "git",
    "url": "https://github.com/imyangyong/vscode-css-to-tailwind"
  },
  "main": "./dist/index.cjs",
  "preview": true,
  "icon": "res/logo.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "contributes": {
    "configuration": {
      "properties": {
        "CssToTailwind.disable": {
          "type": "boolean",
          "default": false,
          "description": "Disable the css-to-tailwind extension"
        }
      }
    },
    "commands": []
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "publish": "vsce publish --no-dependencies",
    "prepare": "nr update",
    "update": "vscode-ext-gen --scope=CssToTailwind --output src/generated/meta.ts",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.1.4",
    "@types/node": "^22.10.10",
    "@types/vscode": "^1.71.0",
    "@vscode/vsce": "^3.2.2",
    "find-up": "^6.3.0",
    "fs-extra": "^11.3.0",
    "jiti": "^2.4.2",
    "prettier": "^2.8.8",
    "transform-to-tailwind": "^0.0.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4",
    "vscode-ext-gen": "^1.0.0"
  },
  "dependencies": {
    "@imyangyong/utils": "^0.10.0"
  }
}
